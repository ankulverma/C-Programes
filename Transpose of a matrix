#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
   { int n[3][3];
   int l,i,j;
    printf("Enter the 3X3 matrix ");
    for(i=0;i<3;i++)
   {


    for(j=0;j<3;j++)
    {
      scanf("%d",&n[i][j]);

    }
   }
   for(i=0;i<3;i++)
   {


    for(j=0;j<3;j++)
    {
      printf("%d  ",n[i][j]);

    }

    printf("\n");
   }
   printf("Press any key for the transpose");
    getch();
   printf("\n Now transpose = \n");
   for(j=0;j<3;j++)
   {

   //printf("\n Now transpose = \n");
    for(i=0;i<3;i++)
    {
      printf("%d  ",n[i][j]);

    }
    printf("\n");
   }


    return 0;
}
