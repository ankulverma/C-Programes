#include <iostream>

using namespace std;

void swap(int x  , int y )
{int t=x;
x=y;
y=t;
cout<<" \n Valuies inside function x  ="<<x<<"and Y="<<y;

}


int main()
  {  //long int fact (int);
  int a,b;
  cout<<"Entyer any two numbers";
  cin>>a>>b;
 
  cout<<"values before calling swap function a="<<a<<"And b ="<<b;
  swap(a,b);
 // int &b=a;
    cout<<"after calling swap a="<<a<<"and b="<<b;
  /*printf("\n x =% d",x);
    printf("\n ADDRESS OF x =% d",&x);
      printf("\n a = %d",a);
