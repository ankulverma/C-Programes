#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
   { char name1[20],name2[20],name3[50];
   int l,i,j;
    printf("Enter name   1\n");
    gets(name1);
    l=strlen(name1);
    j=l;
    for(i=0;i<l;i++)
    {
      j=j-1;
      name2[i]=name1[j];
    }

      name2[i]='\0';
      j=strcmp(name1,name2);
    if(j==0)
    {printf("String is palindrome");}
    else
    printf("String is not palindrome");
    return 0;
}
